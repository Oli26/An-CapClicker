package com.demboiz.ancapclicker.view;

import java.awt.Color;

import javax.swing.JPanel;

public class GamePanel extends JPanel {
	GameMenu menu;
	
	
	
	public GamePanel(){
		this.editor = editor;
    	editor.addObserver(this);
    	setBackground(new Color(0x41, 0x76, 0x43));
    	setVisible(true);
    	setOpaque(true);
	}
}
